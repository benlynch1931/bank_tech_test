{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "rules": {
      "eqeqeq": "off",
      "curly": "error",
      "quotes": ["error", "double"],
      "no-console": "off",

    }
}
